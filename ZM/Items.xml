<Items>
<!--Autonomous alarm which will activate when a blowout is imminent.-->
  <Item name="Blowout Alarm" identifier="fn_blowoutalarm" description="An alarm that should be placed alongside Europan Surface Radiation. It will sound off whenever a blowout is imminent." category="Electrical" Tags="smallitem,sound" maxstacksize="8" cargocontaineridentifier="metalcrate" scale="0.5" impactsoundtag="impact_metal_light" pickdistance="0" noninteractable="true">
    <InventoryIcon texture="Content/Items/InventoryIconAtlas.png" sourcerect="128,127,64,64" origin="0.5,0.5" />
    <Sprite texture="Content/Items/Electricity/signalcomp.png" depth="0.8" sourcerect="75,133,42,57" origin="0.5,0.5" />
    <Body radius="20" density="20" />
    <Holdable selectkey="Select" pickkey="Use" slots="Any,RightHand,LeftHand" msg="ItemMsgDetachWrench" PickingTime="5.0" aimpos="65,-10" handle1="0,0" attachable="true" aimable="true">
      <RequiredItem items="wrench" type="Equipped" />
    </Holdable>
    <LightComponent canbeselected="false" lightcolor="255,0,0,100" castshadows="false" blinkfrequency="2.6" range="20">
      <Sprite texture="Content/Items/Electricity/signalcomp.png" sourcerect="301,963,42,57" origin="0.5,0.5" alpha="1.0"/>
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/blowout/Call of Pripyat/blowout_siren.ogg" type="OnActive" range="10000.0" loop="True" volume="0.5" />
        <!--60 seconds after ambient phase begins, begin alarm sounds
        This will cause it to begin playing right in time for impact.-->
      <StatusEffect type="Always" target="This" delay="60" setvalue="True" checkconditionalalways="True" IsActive="True" stackable="false" >
        <Conditional condition="eq 50" />
      </StatusEffect>
      <!--When the blowout siren sound ends, set condition to zero and deactivate the alarm.-->
      <StatusEffect type="Always" target="This" delay="53" comparison="And" checkconditionalalways="True" setvalue="True" condition="0" IsActive="False" stackable="false" >
        <Conditional IsActive="True" />
        <Conditional condition="eq 50" />
      </StatusEffect>
    </LightComponent>
  </Item>

