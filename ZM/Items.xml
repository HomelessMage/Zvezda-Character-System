<!--Spent waaay too long on this shit, I hope you like it-->
  <Item name="Europan Surface Radiation" identifier="fn_surfaceradiation" category="Equipment" cargocontaineridentifier="metalcrate" tags="smallitem" scale="0.5" description="When placed at the top of a map, this simulates Europan Surface Radiation, alongside periodic blowout sequences which have the potential to zombify people if they die during one. Avoiding a blowout sequence is simple: get inside." pickdistance="0" noninteractable="true">
    <sprite name="rad" texture="Content/Items/Jobgear/headgears.png" sourcerect="0,0,1,1" origin="0.4,0.3" />
    <Body width="40" radius="30" density="15" bodytype="Kinematic" />
    <LightComponent lightcolor="255,255,150,0" range="1" powerconsumption="0" allowingameediting="false" castshadows="false" IsOn="true">
    <!--Every 0.5 seconds, produce a massive-ranged explosion with barely any force that gives a small amount of an affliction
    This affliction checks for thalassophobia radiation suits or pressure protection, and if neither are present radiation sickness is applied
    Explosions have weakened effects past doors and walls, allowing indoor areas to be safe from radiation.-->
      <StatusEffect type="Always" target="This" delay="0.5" stackable="false">
        <Explosion range="10000" force="0.1" flash="false" shockwave="false" sparks="false" flames="false" smoke="false" camerashake="0" underwaterbubble="true" structuredamage="0" itemdamage="0" flashrange="0" flashduration="0">
          <Affliction identifier="fn_radcheck" strength="5" />
        </Explosion>
      </StatusEffect>
      <!--BLOWOUT STUFF START-->
      <!--Reduce condition of all active emitters and blowout alarms to 50 every 15 minutes 
      Other statuseffects check for this condition value
      Only run if condition is greater than 50-->
      <StatusEffect type="Always" target="NearbyItems, This" targetidentifiers="fn_blowoutalarm, fn_surfaceradiation" range="10000" delay="900" setvalue="true" condition="50" stackable="false" >
        <Conditional condition="gt 50" />
      </StatusEffect>
      <!--2 minutes after condition is reduced and blowout ambience announces imminent doom, reduce condition to 2
      Other statuseffects check for this condition value-->
      <StatusEffect type="Always" target="This" delay="120" setvalue="true" condition="2" stackable="false" >
        <Conditional condition="eq 50" />
      </StatusEffect>
      <!--30 seconds after a blowout sequence ends, set condition to 1 and play spooky ambience as the menacing sounds slowly fall silent-->
      <StatusEffect type="Always" target="This" delay="30" setvalue="true" condition="1" stackable="false" >
        <Conditional condition="eq 0" />
      </StatusEffect>
      <!--Reset emitter to full condition, next blowout in 15 minutes-->
      <StatusEffect type="Always" target="This" delay="70" setvalue="true" condition="100" stackable="false" >
        <Conditional condition="eq 1" />
      </StatusEffect>
      <!--Ambient sounds to announce imminent danger when condition is 50-->
      <StatusEffect type="Always" target="This" delay="17" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient1.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient2.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient3.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient4.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient5.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Conditional condition="eq 50" />
      </StatusEffect>
      <!--Spookier sounds that happen less frequently when condition is 50-->
      <StatusEffect type="Always" target="This" delay="27" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/announce1.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/announce2.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Conditional condition="eq 50" />
      </StatusEffect>
      <!--Sound that plays just before a blowout impacts-->
      <StatusEffect type="Always" target="This" delay="113" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/prehit.ogg" range="10000" volume="10" dontmuffle="True" />
        <Conditional condition="eq 50" />
      </StatusEffect>
      <!--Sound that plays when a blowout sequence ends-->
      <StatusEffect type="Always" target="This" delay="4" setvalue="true" condition="1" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/posthit.ogg" range="10000" volume="10" dontmuffle="True" />
        <Conditional condition="eq 4" />
      </StatusEffect>
      <!--Impact sound-->
      <StatusEffect type="Always" target="This" delay="0.5" setvalue="true" condition="0" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/start.ogg" range="10000" volume="10" dontmuffle="True" />
        <Conditional condition="eq 2" />
        <Explosion range="10000" force="0.1" flash="false" shockwave="false" sparks="false" flames="false" smoke="false" camerashake="143" underwaterbubble="true" structuredamage="0" itemdamage="0" flashrange="10000" flashduration="1">
          <Affliction identifier="stun" strength="0.1" />
        </Explosion>
      </StatusEffect>
      <!--Apply affliction to everyone within 1000 meters or so, for the spooky visual effects-->
      <StatusEffect type="Always" target="NearbyCharacters, This" range="10000" disabledeltatime="true" stackable="false" >
        <Affliction identifier="fn_blowoutimpact" amount="1000" />
        <Conditional condition="eq 2" />
      </StatusEffect>
      <!--Buildup sound that increases in intensity after a blowout hits-->
      <StatusEffect type="Always" target="This" delay="2" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/buildup.ogg" range="10000" volume="10" dontmuffle="True" />
        <Conditional condition="eq 0" />
      </StatusEffect>
      <!--Applies stun and affliction to all people within ~1000 meters
      As long as the person is not outside, they will not gain the vitality check affliction and survive scot-free-->
      <StatusEffect type="Always" target="NearbyCharacters, This" range="10000" delay="2.5" >
        <Affliction identifier="stun" amount="5" />
        <Affliction identifier="fn_blowoutsurvive" amount="10000" />
        <ReduceAffliction identifier="fn_blowout" amount="10000" />
        <Conditional condition="eq 4" />
      </StatusEffect>
      <!--Applies an affliction after condition is 50, which slowly applies visual effects to give the idea that something bad is about to happen.-->
      <StatusEffect type="Always" target="NearbyCharacters, This" range="10000" >
        <Affliction identifier="fn_blowout" amount="1.1" />
        <Conditional condition="eq 50" />
      </StatusEffect>
      <!--Psi-wave sound which sets condition to 4-->
      <StatusEffect type="Always" target="This" delay="17" setvalue="true" condition="4" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/end.ogg" range="10000" volume="10" dontmuffle="True" />
        <Conditional condition="eq 0" />
      </StatusEffect>
      <!--Anyone outside gets severe burns, if these burns land them in crit they will die on the spot and eventually get back up-->
      <StatusEffect type="Always" target="This" delay="2.5" duration="3" stackable="false" >
        <Explosion range="10000" force="0.1" flash="false" shockwave="false" sparks="false" flames="false" smoke="false" camerashake="125" underwaterbubble="true" structuredamage="0" itemdamage="0" flashrange="10000" flashduration="1.5">
          <Affliction identifier="fn_blowoutcheck" strength="5" />
        </Explosion>
        <Conditional condition="eq 4" />
      </StatusEffect>
      <!--Visual effects that sync up with lightning sounds-->
      <StatusEffect type="Always" target="NearbyCharacters, This" delay="6" disabledeltatime="true" range="10000" stackable="false" >
          <Affliction identifier="fn_blowoutlightning" amount="50" />
        <Conditional condition="eq 0" />
      </StatusEffect>
      <StatusEffect type="Always" target="NearbyCharacters, This" delay="7" disabledeltatime="true" range="10000" stackable="false" >
          <Affliction identifier="fn_blowoutlightning" amount="50" />
        <Conditional condition="eq 0" />
      </StatusEffect>
      <StatusEffect type="Always" target="NearbyCharacters, This" delay="9" disabledeltatime="true" range="10000" stackable="false" >
          <Affliction identifier="fn_blowoutlightning" amount="50" />
        <Conditional condition="eq 0" />
      </StatusEffect>
      <!--Lightning sounds that sync up with visual effects-->
      <StatusEffect type="Always" target="This" delay="6" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit1.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit2.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit3.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Conditional condition="eq 0" />
        <Explosion range="10000" force="0.1" flash="false" shockwave="false" sparks="false" flames="false" smoke="false" camerashake="3" underwaterbubble="true" structuredamage="0" itemdamage="0" flashrange="10000" flashduration="0.5">
          <Affliction identifier="stun" strength="0.1" />
        </Explosion>
      </StatusEffect>
      <StatusEffect type="Always" target="This" delay="9" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit1.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit2.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit3.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Conditional condition="eq 0" />
        <Explosion range="10000" force="0.1" flash="false" shockwave="false" sparks="false" flames="false" smoke="false" camerashake="3" underwaterbubble="true" structuredamage="0" itemdamage="0" flashrange="10000" flashduration="0.5">
          <Affliction identifier="stun" strength="0.1" />
        </Explosion>
      </StatusEffect>
      <StatusEffect type="Always" target="This" delay="7" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit1.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit2.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/hit3.ogg" range="10000" volume="10" dontmuffle="True" selectionmode="Random" />
        <Conditional condition="eq 0" />
        <Explosion range="10000" force="0.1" flash="false" shockwave="false" sparks="false" flames="false" smoke="false" camerashake="3" underwaterbubble="true" structuredamage="0" itemdamage="0" flashrange="10000" flashduration="0.5">
          <Affliction identifier="stun" strength="0.1" />
        </Explosion>
      </StatusEffect>
      <!--Ambient sound effects which play after a blowout ends-->
      <StatusEffect type="Always" target="This" delay="13" stackable="false" >
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient1.ogg" range="10000" volume="0.5" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient2.ogg" range="10000" volume="0.5" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient3.ogg" range="10000" volume="0.5" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient4.ogg" range="10000" volume="0.5" dontmuffle="True" selectionmode="Random" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Blowout/ambient5.ogg" range="10000" volume="0.5" dontmuffle="True" selectionmode="Random" />
        <Conditional condition="eq 1" />
      </StatusEffect>
      <!--Emitter will eventually reset its condition and a new blowout happens 15 minutes later.-->
    </LightComponent>
  </Item>
