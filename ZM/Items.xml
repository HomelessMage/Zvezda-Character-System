        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Gore/WoodBat-3.ogg" range="5000" volume="0.5" />
        <Sound file="Mods/Zvezda Character System/ZM/Sounds/Gore/WoodBat-4.ogg" range="5000" volume="0.5" />
